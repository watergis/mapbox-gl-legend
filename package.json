{
  "name": "@watergis/mapbox-gl-legend",
  "version": "1.2.0",
  "description": "This module adds legend control which is able to create legend panel from mapbox style to mapbox-gl",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "browser": "dist/cdn/mapbox-gl-legend.js",
  "files": [
    "dist",
    "css"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc && npm run build:cdn && npm run build:example",
    "build:example": "webpack --mode production --config webpack.config.js",
    "build:cdn": "webpack --mode production --config webpack.cdn.config.js && cp ./dist/cdn/mapbox-gl-legend.js ./docs/. && cp ./css/styles.css ./docs/mapbox-gl-legend.css",
    "dev": "webpack serve --mode development --open --hot --inline --config webpack.config.js",
    "deploy": "gh-pages -d docs -m 'deploy to gh-pages'"
  },
  "keywords": [
    "mapbox",
    "mapbox-gl-js",
    "legend"
  ],
  "author": "Jin IGARASHI",
  "license": "MIT",
  "devDependencies": {
    "@types/mapbox-gl": "^1.13.0",
    "css-loader": "^3.6.0",
    "gh-pages": "^3.2.3",
    "path": "^0.12.7",
    "style-loader": "^1.3.0",
    "ts-loader": "^8.0.17",
    "typescript": "^3.9.9",
    "webpack": "^5.15.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@mapbox/mapbox-gl-style-spec": "^13.19.0",
    "@watergis/legend-symbol": "^0.2.2",
    "axios": "^0.21.1",
    "mapbox-gl": "^1.13.1"
  }
}
